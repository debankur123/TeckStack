[*.cs]

# CS8618: Non-nullable field must contain a non-null value when exiting constructor. Consider declaring as nullable.
dotnet_diagnostic.CS8618.severity = silent

# CS8602: Dereference of a possibly null reference.
dotnet_diagnostic.CS8602.severity = silent

# IDE0017: Simplify object initialization
dotnet_diagnostic.IDE0017.severity = silent

# CS8601: Possible null reference assignment.
dotnet_diagnostic.CS8601.severity = silent

# CS0168: Variable is declared but never used
dotnet_diagnostic.CS0168.severity = silent

# IDE0011: Add braces
dotnet_diagnostic.IDE0011.severity = silent

# CS0472: The result of the expression is always the same since a value of this type is never equal to 'null'
dotnet_diagnostic.CS0472.severity = silent
